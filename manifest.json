{
	"manifest_version": 2,
	"name": "Search in Dictionary",
	"description": "Search for a word on a web page in a dictionary",
	"version": "1.0",
	"icons": {"128": "icon_128.png"},
	"browser_action": {"default_icon": "icon.png"},
	"permissions": ["contextMenus", "tabs"],
	"background": {
		"scripts": ["background.js"]
	}
}